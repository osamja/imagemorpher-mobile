{
  "expo": {
    "name": "MyMorph",
    "slug": "ImageMorpherMobile",
    "description": "Morph two faces together",
    "version": "3.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "jsEngine": "hermes",
    "plugins": [
      "expo-apple-authentication"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "background": "linear-gradient(#e0c3fc, #8ec5fc)"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.sammyjaved.imagemorpher",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
    },
    "extra": {
      "eas": {
        "projectId": "24c2031d-2ad9-49de-9882-0c118b825a80"
      }
    },
    "owner": "air217"
  }
}
